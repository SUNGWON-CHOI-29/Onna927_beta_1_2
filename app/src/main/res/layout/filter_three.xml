<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
        <ImageView
            android:layout_width="264dp"
            android:layout_height="28dp"
            android:id="@+id/menu_content"
            android:layout_marginTop="30dp"
            android:src="@drawable/filter_three_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:layout_marginTop="30dp"
                android:id="@+id/button"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/menu_content"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_02_set"
                android:background="@android:color/transparent"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button1"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_01_set"
                android:background="@android:color/transparent"
                android:layout_alignTop="@+id/button"
                android:layout_toLeftOf="@+id/button3"
                android:layout_toStartOf="@+id/button3" />
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button2"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_03_set"
                android:background="@android:color/transparent"
                android:layout_alignTop="@+id/button"
                android:layout_toRightOf="@+id/button3"
                android:layout_toEndOf="@+id/button3" />
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:layout_margin="20dp"
                android:id="@+id/button3"
                android:layout_below="@id/button2"
                android:layout_centerHorizontal="true"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_05_set"
                android:background="@android:color/transparent"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button4"
                android:layout_alignTop="@+id/button3"
                android:layout_alignLeft="@+id/button1"
                android:layout_alignStart="@+id/button1"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_04_set"
                android:background="@android:color/transparent"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:layout_margin="20dp"
                android:id="@+id/button5"
                android:layout_below="@+id/button2"
                android:layout_toRightOf="@+id/button"
                android:layout_toEndOf="@+id/button"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_06_set"
                android:background="@android:color/transparent"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button6"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_08_set"
                android:background="@android:color/transparent"
                android:layout_below="@+id/button3"
                android:layout_alignLeft="@+id/button3"
                android:layout_alignStart="@+id/button3" />
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button7"
                android:layout_alignTop="@+id/button6"
                android:layout_alignLeft="@+id/button4"
                android:layout_alignStart="@+id/button4"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_07_set"
                android:background="@android:color/transparent"/>
            <ImageButton
                android:layout_width="82dp"
                android:layout_height="95dp"
                android:id="@+id/button8"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/food_09_set"
                android:background="@android:color/transparent"
                android:layout_below="@+id/button5"
                android:layout_toRightOf="@id/button3"
                android:layout_toEndOf="@id/button3" />

            <ImageView
                android:layout_width="94dp"
                android:layout_height="10dp"
                android:id="@+id/menu_notice"
                android:layout_below="@id/button7"
                android:src="@drawable/bid_1_detail_08"
                android:layout_marginTop="30dp"
                android:layout_centerHorizontal="true" />
        <ImageButton
            android:layout_width="73dp"
            android:layout_height="19dp"
            android:id="@+id/button9"
            android:padding="0sp"
            android:scaleType="centerCrop"
            android:src="@drawable/bid_1_detail_09"
            android:layout_below="@id/menu_notice"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:background="@android:color/transparent"/>
            <TextView
                android:id="@+id/filter_three_pick"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_alignBottom="@id/menu_content"
                android:layout_alignLeft="@id/menu_content"
                android:layout_alignRight="@id/menu_content"
                android:layout_alignTop="@id/menu_content"
                android:gravity="center"

                android:paddingLeft="55dp"
                android:textColor="#000000"
                android:textSize="10dp"
                android:textStyle="bold"
                android:typeface="sans" />
</RelativeLayout>